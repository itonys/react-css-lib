{
  "name": "reset-css-lib",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.18",
    "react-dom": "^18.3.1",
    "sass": "^1.77.2"
  },
  "scripts": {
    "build:reset": "sass --no-source-map src/token/main.scss dist/reset.css",
    "build:atom": "sass --no-source-map src/atom/main.scss dist/atom.css",
    "build:pattern": "sass --no-source-map src/pattern/main.scss dist/pattern.css",
    "clean": "rm -rf dist",
    "build": "yarn build:reset && yarn build:atom && yarn build:pattern",
    "watch": "nodemon --watch src --ext scss --exec 'yarn build'"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
